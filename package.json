{
  "name": "react-awesome-form-validator",
  "version": "1.3.0",
  "description": "This is a very nice form with validations using ReactJs",
  "main": "./dist/index.jsx",
  "keywords": [
    "react",
    "form validation",
    "react form validation"
  ],
  "scripts": {
    "build": "babel-compile -p es2015 -p react -p stage-0 src/form:dist",
    "compile": "npm run build",
    "postinstall": "npm run build",
    "test": "NODE_ENV=test gulp test",
    "lint": "eslint . --fix",
    "start": "exampleFolder=simple-form node ./node_modules/webpack-dev-server/bin/webpack-dev-server --port 3000 --host 0.0.0.0 --config webpack.config.dev.js --hot --progress --inline"
  },
  "pre-push": [
    "lint",
    "test"
  ],
  "author": "Damian Aruj <aruj.damian@gmail.com>",
