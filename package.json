{
  "name": "react-awesome-form-validator",
  "version": "1.3.8",
  "description": "This is a very nice form with validations using ReactJs",
  "main": "./dist/index.js",
  "keywords": [
    "react",
    "form validation",
    "react form validation"
  ],
  "scripts": {
    "compile": "babel ./src/form --out-dir dist --copy-files --source-maps --presets=latest,stage-0,react",
    "test": "NODE_ENV=test gulp test",
    "lint": "eslint . --fix",
    "start": "npm run simple-form",
    "prepublish": "npm run lint && npm run test && npm run compile",
    "simple-form": "exampleFolder=simple-form node ./node_modules/webpack-dev-server/bin/webpack-dev-server --port 3000 --host 0.0.0.0 --config webpack.config.babel.js --hot --progress --inline",
    "form-with-reset-and-clear-values": "exampleFolder=form-with-reset-and-clear-values node ./node_modules/webpack-dev-server/bin/webpack-dev-server --port 3000 --host 0.0.0.0 --config webpack.config.babel.js --hot --progress --inline",
    "form-with-custom-input": "exampleFolder=form-with-custom-input node ./node_modules/webpack-dev-server/bin/webpack-dev-server --port 3000 --host 0.0.0.0 --config webpack.config.babel.js --hot --progress --inline"
  },
  "pre-push": [
    "lint",
    "test"
  ],
  "author": "Damian Aruj <aruj.damian@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/daruj/react-awesome-form-validator.git"
  },
  "bugs": {
    "url": "https://github.com/daruj/react-awesome-form-validator/issues"
  },
  "license": "ISC",
  "dependencies": {
    "classnames": "2.2.5",
    "lodash": "4.16.4",
    "react": "^15.4.2",
    "react-click-outside": "2.2.0",
    "react-dom": "^15.4.2",
    "validator": "6.2.0"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "^6.23.0",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.16.0",
    "babel-preset-airbnb": "2.1.1",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.23.0",
    "chai": "3.5.0",
    "css-loader": "0.25.0",
    "css-modules-require-hook": "4.0.5",
    "debug": "2.2.0",
    "enzyme": "2.7.1",
    "eslint": "3.8.1",
    "eslint-loader": "1.6.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "0.9.0",
    "gulp": "3.9.1",
    "gulp-mocha": "3.0.1",
    "html-webpack-plugin": "2.24.0",
    "jsdom": "9.9.1",
    "node-sass": "4.5.0",
    "pre-push": "0.1.1",
    "react-addons-test-utils": "15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "require-dir": "0.3.1",
    "sass-loader": "6.0.2",
    "style-loader": "0.13.1",
    "superagent": "2.3.0",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1",
    "webpack-notifier": "1.4.1"
  }
}
